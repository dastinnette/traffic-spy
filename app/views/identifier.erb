<div class="jumbotron">
   <h1><%= @identifier.identifier %></h1>
   <br>
   <p>
     <% url_list = [] %>
     <% response_list = []%>
     <% @identifier.payloads.map do |payload| %>
       <% url_list << payload.url %>
       <% response_list << payload.responded_in %>
       <% response_list %>
       <% url_list %> </p>
       <% %>
     <% end %>
      <% count_of_urls = url_list.inject(Hash.new(0)) {|total, e| total[e] += 1 ;total}.sort_by {|key, value| -value} %>
      <% count_of_urls.each do |url, count| %>
       <p>
        <%= url %>
        <%=": Was requested #{count} time(s)" %>
       </p>
       <br>
      <% end %>
      <% count_of_responses = response_list.inject(Hash.new(0)) {|total, e| total[e] += 1 ;total}.sort_by {|key, value| -value} %>
      <% count_of_responses.each do |response, count| %>
       <p>
        <%= response %>
        <%=": Was requested #{count} time(s)" %>
       </p>
       <br>
      <% end %>
    </p>
    <p>
      <% @identifier.payloads.map do |payload| %>
        <%= browser_output(payload.user_agent_string) %>
      <% end %>
    </p>
    <p>
      <% @identifier.payloads.map do |payload| %>
        <%= platform_output(payload.user_agent_string) %>
      <% end %>
      <%= @something %>
    </p>
    <p>
      <% @resolution_source.to_a.map do |resolution| %>
        <%= resolution.height %>
        <%= resolution.width %>
      <% end %>
    </p>
</div>
